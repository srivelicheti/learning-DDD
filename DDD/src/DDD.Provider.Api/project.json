{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "outputName": "DDD.Provider.Api"
  },
  "frameworks": {
    "net461": {
      "dependencies": {
        "log4net": "2.0.5",
        "DDD.Provider.DataModel": "1.0.0-*",
        "AutoMapper": "4.2.1",
        "DDD.Provider.QueryStack": "1.0.0-*",
        "NServiceBus": "5.2.14",
        "NServiceBus.NHibernate": "6.2.7"
      },
      "frameworkAssemblies": {
        //"System.Collections": "4.0.10.0",
        "System.Configuration": "4.0.0.0"
      }
    }
  },
  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "appsettings.json",
      "config.json",
      "web.config"
    ]
  },
  "runtimeOptions": {
    "gcServer": true
  },
  "scripts": {
    "prepublish": [ "npm install", "bower install", "gulp clean", "gulp min" ],
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  },
  "tools": {
  },
  "dependencies": {
    "DDD.Common": "1.0.0-*",
    "DDD.Domain.Common": "1.0.0-*",
    "DDD.Provider.Api.Contracts": "1.0.0-*",
    "DDD.Provider.Domain": "1.0.0-*",
    "DDD.Provider.Domain.Contracts": "1.0.0-*",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Microsoft.AspNetCore.Mvc": "1.0.1",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.1",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.1",
    "Microsoft.AspNetCore.SignalR.Server": "0.1.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.Logging": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.Extensions.Logging.Debug": "1.0.0",
    "Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0",
    "Newtonsoft.Json": "9.0.1",
    "StructureMap.Microsoft.DependencyInjection": "1.2.0",
    "Microsoft.EntityFrameworkCore": "1.0.1",
    "Microsoft.EntityFrameworkCore.SqlServer": "1.0.1",
    "Microsoft.EntityFrameworkCore.SqlServer.Design": "1.0.1"
  }
}
